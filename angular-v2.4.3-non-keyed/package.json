{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for angular 2",
  "scripts": {
    "start": "npm run build-dev && npm run serve",
    "test": "karma start",
    "precompile": "ngc",
    "build-prod": "npm run precompile && node rollup.config.js --prod",
    "build-dev": "npm run precompile && node rollup.config.js",
    "postinstall": "typings install"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/compiler-cli": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/platform-server": "2.4.3",
    "core-js": "2.4.1",
    "rxjs": "5.0.3",
    "zone.js": "0.7.5"
  },
  "devDependencies": {
    "google-closure-compiler-js": "20161201.0.0",
    "http-server": "0.9.0",
    "rollup": "0.41.4",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "typescript": "2.1.5",
    "typings": "2.1.0",
    "yargs": "6.6.0"
  }
}
