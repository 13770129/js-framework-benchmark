{
  "name": "js-framework-benchmark-incr_dom",
  "version": "1.0.0",
  "description": "Incr_dom version of benchmark",
  "author": "Michael Boulton",
  "license": "Apache-2.0",
  "homepage": "https://github.com/krausest/js-framework-benchmark",
  "repository": {
    "type": "git",
    "url": "https://github.com/krausest/js-framework-benchmark.git"
  },
  "keywords": [
    "webpack",
    "ReasonML",
    "incr_dom"
  ],
  "esy": {
    "build": "refmterr dune build -p js-framework-benchmark -j 3"
  },
  "scripts": {
    "run": "esy x bash -c 'http-server #{self.bin}'",
    "build": "esy b",
    "build:bucklescript": "bsb -make-world",
    "test": "refmterr esy b dune runtest --force",
    "dbg": "refmterr esy b dune build ./test/RunTests.bc"
  },
  "peerDependencies": {
    "ocaml": "^4.7.0"
  },
  "dependencies": {
    "@opam/incr_dom": "v0.12.0",
    "@reason-native/console": "*"
  },
  "devDependencies": {
    "http-server": "*",
    "@esy-ocaml/reason": "*",
    "@opam/merlin": "*",
    "refmterr": "*"
  }
}
