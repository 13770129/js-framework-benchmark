{
  "name": "js-framework-benchmark-angular-closure",
  "version": "1.0.0",
  "js-framework-benchmark": {
    "frameworkVersionFromPackage": "@angular/core"
  },
  "description": "An angular app using closure compiler",
  "scripts": {
    "postinstall": "npm run ngcc",
    "start": "echo 'please start in root project'",
    "test": "karma start",
    "ngc": "ngc",
    "ngcc": "ngcc",
    "preclosure": "rimraf dist",
    "closure": "google-closure-compiler --flagfile closure.conf",
    "build-prod": "npm run ngc && npm run closure",
    "rollup": "rollup -c rollup.config.js",
    "build-dev": "npm run ngc && npm run rollup"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "11.0.2",
    "@angular/compiler": "11.0.2",
    "@angular/core": "11.0.2",
    "@angular/platform-browser": "11.0.2",
    "core-js": "^3.8.1",
    "rxjs": "6.5.5",
    "tsickle": "0.39.1",
    "tslib": "^2.0.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular/compiler-cli": "11.0.2",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-node-resolve": "8.0.1",
    "google-closure-compiler": "20201207.0.0",
    "http-server": "0.12.1",
    "rimraf": "3.0.1",
    "rollup": "2.13.1",
    "rollup-plugin-terser": "6.1.0",
    "typescript": "4.0.5"
  }
}
