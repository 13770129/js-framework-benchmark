    "@types/selenium-webdriver": "4.0.15",
    "@types/semver": "7.3.9",
    "@types/yargs": "17.0.4",
    "dockerode": "^3.3.1",
    "rimraf": "^3.0.2",
    "ts-node": "10.3.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "chromedriver": "^94.0.0",
    "cross-env": "7.0.3",
    "dot": "1.1.3",
    "jstat": "1.9.5",
    "lighthouse": "8.5.1",
    "lodash": "4.17.21",
    "marky": "^1.2.2",
    "npm-check-updates": "11.8.5",
    "ramda": "0.27.1",
    "selenium-webdriver": "4.0.0",
    "semver": "7.3.5",
    "yargs": "17.2.1"
  }
